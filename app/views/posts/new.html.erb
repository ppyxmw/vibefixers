<h2>Create a new post</h2>
<div>
  <label for="search">Search & select track</label> <br>
  <input type='text' id="search" onchange='doRender()' />
  <input type="submit" value="Search">
</div> <br>
<div id='catalog'></div> <br>

<div>
  <%= form_for @post do |f| %>
    <%= f.label :title %> <br>
    <%= f.text_area :title %> <br>
    <%= f.label :tags %> <br>
    <%= f.text_area :tags %> <br>
    <%= f.label :summary %> <br>
    <%= f.text_area :summary %> <br>
    <%= f.label :description %> <br>
    <%= f.text_area :description %> <br>
    <%= f.hidden_field :artwork, value: "http://tinyurl.com/n9lcatp" %>
    <%= f.submit 'Publish' %>
  <% end %>
</div>
